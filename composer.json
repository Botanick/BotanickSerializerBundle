{
    "name": "botanick/serializer-bundle",
    "type": "symfony-bundle",
    "description": "Serialize your structures into PHP primitives (scalars, arrays of scalars, arrays of arrays).",
    "keywords": [
        "serialization",
        "object",
        "structure",
        "array"
    ],
    "homepage": "https://github.com/Botanick/BotanickSerializerBundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Andrey \"Botanick\" Chushikin",
            "homepage": "https://github.com/Botanick"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "symfony/framework-bundle": "~2.3",
        "botanick/serializer": "^1.0.0"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "symfony/dependency-injection": "~2.3",
        "symfony/config": "~2.3",
        "symfony/http-kernel": "~2.3",
        "symfony/filesystem": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Botanick\\SerializerBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
